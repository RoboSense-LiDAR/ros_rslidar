<launch>
    <node  name="rslidar_node" pkg="rslidar_driver" type="rslidar_node" output="screen" >
    <param name="pcap" value="/home/tony-sti/lidar_data/lm75-170616-roadtest.pcap"/>
    <!--param name="device_ip" value="192.168.1.200"-->
    <!-- Set the Lidar data port on the PC side, default is 6699 -->
	<param name="port" value="6699"/>
	</node>

     <node  name="cloud_node" pkg="rslidar_pointcloud" type="cloud_node" output="screen" >
     <param name="pwr_path" value="/data/LM79/pwrCurves.csv" />
	 <param name="curves_path" value="/data/LM79/curves.csv" />
	 <param name="angle_path" value="/data/LM79/angle.csv" />
	 <param name="channel_path" value="/data/LM79/ChannelNum.csv" />
	  </node>

</launch>
